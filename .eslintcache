 React, { useContext, useEffect } from 'react';\r\nimport './Navigation.scss';\r\nimport { Link, useHistory } from 'react-router-dom';\r\nimport { ReactComponent as Forge } from '../../assets/icons/icons8-hammer-and-anvil.svg';\r\nimport { UserContext } from '../../context/UserContext';\r\nimport axios from 'axios';\r\n\r\nexport default function Navigation() {\r\n\r\n    const history = useHistory();\r\n\r\n    const { user, handleAuth } = useContext(UserContext);\r\n\r\n    const handleLogout = () => {\r\n        localStorage.removeItem('FBIdToken');\r\n        delete axios.defaults.headers.common['Authorization'];\r\n        handleAuth(null, true);\r\n        history.push('/');\r\n    }\r\n\r\n    return (\r\n        <nav className=\"nav\">\r\n            <div className=\"nav__wrapper\">\r\n                <div className=\"nav__sub\">\r\n                    <Link to='/' className=\"link\">\r\n                    <Forge className=\"logo\"/>\r\n                    <div className=\"nav__link\">\r\n                        {(user ? `${user.credentials.name}'s` : \"\")}  Forge\r\n                    </div>\r\n                    </Link>\r\n                    { user ?             \r\n                    <Link to='/kiln' className=\"link\">\r\n                    <div className=\"nav__link\">\r\n                        Kiln\r\n                    </div>\r\n                    </Link >: \"\"}\r\n                </div>\r\n                \r\n                { user ?\r\n                <div className=\"link nav__link\" onClick={handleLogout}>\r\n                Log Out\r\n                </div> \r\n                : \r\n                <Link to='/join' className=\"link\">\r\n                    <div className=\"nav__link\">\r\n                    Join\r\n                    </div>\r\n                </Link>}\r\n            </div>\r\n        </nav>\r\n    )\r\n}\r\n","C:\\Users\\amier\\OneDrive\\Desktop\\PathOfCode\\Brainstation\\forge-v2\\src\\components\\pages\\Home\\Home